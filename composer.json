{
    "name": "ixocreate/framework",
    "description": "IXOCREATE Framework",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE",
            "email": "opensource@ixocreate.com",
            "homepage": "https://www.ixocreate.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2 || ^8.0",
        "filp/whoops": "^2.1",
        "ixocreate/admin-package": "dev-develop",
        "ixocreate/application": "dev-develop",
        "ixocreate/cms-package": "0.4.x-dev",
        "ixocreate/mail-package": "dev-develop",
        "ixocreate/media-package": "dev-develop",
        "ixocreate/registry-package": "dev-develop",
        "ixocreate/translation-package": "dev-develop"
    },
    "require-dev": {
        "ixocreate/coding-standard": "^2.0",
        "ixocreate/test": "^0.1"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Framework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ixocreate\\Misc\\Framework\\": "tests/misc/",
            "Ixocreate\\Test\\Framework\\": "tests/src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
