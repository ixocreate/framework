{
    "name": "ixocreate/framework",
    "description": "IXOCREATE framework composition",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "ixocreate/application-console": "dev-develop",
        "ixocreate/application-http": "dev-develop",
        "ixocreate/asset": "dev-develop",
        "ixocreate/cache-package": "dev-develop",
        "ixocreate/cms": "dev-develop",
        "ixocreate/command-bus": "dev-develop",
        "ixocreate/command-bus-filter": "dev-develop",
        "ixocreate/command-bus-validation": "dev-develop",
        "ixocreate/common-types": "dev-develop",
        "ixocreate/config": "dev-develop",
        "ixocreate/database": "dev-develop",
        "ixocreate/entity": "dev-develop",
        "ixocreate/event": "dev-develop",
        "ixocreate/filesystem": "dev-develop",
        "ixocreate/filter": "dev-develop",
        "ixocreate/intl": "dev-develop",
        "ixocreate/media": "dev-develop",
        "ixocreate/project-uri": "dev-develop",
        "ixocreate/registry": "dev-develop",
        "ixocreate/resource": "dev-develop",
        "ixocreate/schema": "dev-develop",
        "ixocreate/template": "dev-develop",
        "ixocreate/translation": "dev-develop",
        "ixocreate/validation": "dev-develop",
        "ixocreate/servicemanager": "dev-develop"
    },
    "require-dev": {
        "ixocreate/coding-standard": "^1.0",
        "php-coveralls/php-coveralls": "^2.0",
        "phpunit/phpunit": "^6.0.6"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Framework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IxocreateMisc\\Framework\\": "tests/misc/",
            "IxocreateTest\\Framework\\": "tests/src/"
        }
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
