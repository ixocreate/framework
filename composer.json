{
    "name": "kiwi-suite/framework",
    "description": "kiwi-suite/framework",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Thomas Payer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "kiwi-suite/servicemanager": "dev-develop",
        "kiwi-suite/entity": "dev-develop",
        "kiwi-suite/application-http": "dev-develop",
        "kiwi-suite/application-console": "dev-develop",
        "kiwi-suite/common-types": "dev-develop",
        "kiwi-suite/config": "dev-develop",
        "kiwi-suite/database": "dev-develop",
        "kiwi-suite/project-uri": "dev-develop",
        "kiwi-suite/command-bus": "v2.x-dev",
        "kiwi-suite/command-bus-validation": "dev-develop",
        "kiwi-suite/command-bus-filter": "dev-develop",
        "kiwi-suite/asset": "dev-develop",
        "kiwi-suite/filesystem": "dev-develop",
        "kiwi-suite/media": "v2.x-dev",
        "kiwi-suite/cms": "v2.x-dev",
        "kiwi-suite/intl": "dev-develop",
        "kiwi-suite/schema": "dev-develop",
        "kiwi-suite/scheduler": "dev-develop",
        "kiwi-suite/template": "dev-develop",
        "kiwi-suite/translation": "dev-develop",
        "kiwi-suite/event": "dev-develop",
        "kiwi-suite/validation": "dev-develop",
        "kiwi-suite/filter": "dev-develop",
        "kiwi-suite/resource": "dev-develop"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0.6",
        "friendsofphp/php-cs-fixer": "^2.5",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "KiwiSuite\\Framework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KiwiSuiteMisc\\Framework\\": "tests/misc/",
            "KiwiSuiteTest\\Framework\\": "tests/src/"
        }
    }
}
