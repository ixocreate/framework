{
    "name": "ixocreate/framework",
    "description": "IXOCREATE Framework",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "ixocreate/admin-package": "^0.1",
        "ixocreate/application": "^0.1",
        "ixocreate/asset-package": "^0.1",
        "ixocreate/cache-package": "^0.1",
        "ixocreate/cms-package": "^0.1",
        "ixocreate/command-bus-package": "^0.1",
        "ixocreate/database-package": "^0.1",
        "ixocreate/entity-package": "^0.1",
        "ixocreate/event-package": "^0.1",
        "ixocreate/filesystem-package": "^0.1",
        "ixocreate/filter-package": "^0.1",
        "ixocreate/intl-package": "^0.1",
        "ixocreate/mail-package": "^0.1",
        "ixocreate/media-package": "^0.1",
        "ixocreate/registry-package": "^0.1",
        "ixocreate/resource-package": "^0.1",
        "ixocreate/schema-package": "^0.1",
        "ixocreate/template-package": "^0.1",
        "ixocreate/translation-package": "^0.1",
        "ixocreate/type-package": "^0.1",
        "ixocreate/validation-package": "^0.1"
    },
    "require-dev": {
        "ixocreate/coding-standard": "^1.0",
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Framework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ixocreate\\Misc\\Framework\\": "tests/misc/",
            "Ixocreate\\Test\\Framework\\": "tests/src/"
        }
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
